import { createApp, emitLifeCycles, staticConfig, createHistory } from './common';
<% if(isMiniapp){ %>
  import miniappRenderer from 'renderer-miniapp';
<% } else { %>
  import reactRenderer from 'renderer-react';
<% } %>

<% if (globalStyle) {%>
  import '../<%= globalStyle %>'
<% } %>

export function runApp(appConfig) {
  <% if(isMiniapp){ %>
    miniappRenderer({
      appConfig,
      createApp,
      staticConfig,
      createHistory
    });
  <% } else { %>
    reactRenderer({
      appConfig,
      emitLifeCycles,
      createApp,
    });
  <% } %>
};
