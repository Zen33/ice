<% if(isMiniapp){ %>
  import renderMiniApp from './renderMiniApp';
<% } else { %>
  import { renderReactApp } from './renderReactApp';
<% } %>

<% if (globalStyle) {%>
  import '../<%= globalStyle %>'
<% } %>

function runApp(appConfig) {
  <% if(isMiniapp){ %>
    renderMiniApp(appConfig);
  <% } else{ %>
    renderReactApp(appConfig);
  <% } %>
}

export {
  runApp
}
