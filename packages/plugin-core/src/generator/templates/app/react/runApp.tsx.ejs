<% if(isMiniapp){ %>
  import renderMiniapp from './renderMiniapp';
<% } else { %>
  import { render } from './render';
<% } %>

<% if (globalStyle) {%>
  import '../<%= globalStyle %>'
<% } %>

function runApp(appConfig) {
  <% if(isMiniapp){ %>
    renderMiniapp(appConfig);
  <% } else{ %>
    render(appConfig);
  <% } %>
}

export {
  runApp
}
