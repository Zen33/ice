<% if(isMiniapp){ %>
  import miniappRenderer from 'miniapp-renderer';
<% } else { %>
  import reactRenderer from 'react-renderer';
<% } %>
import mod from './common';
<% if (globalStyle) {%>import '../<%= globalStyle %>'<% } %>

export function runApp(appConfig) {
  <% if(isMiniapp){ %>
    mod.initializeApp(appConfig, miniappRenderer);
  <% } else { %>
    mod.initializeApp(appConfig, reactRenderer);
  <% } %>
};
